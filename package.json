{
  "name": "@jaspero/page-builder",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "build:elements": "npm --prefix web-components run build && rm -rf static/elements && cp -r web-components/dist static/elements",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "gh-pages": "gh-pages -d build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@jaspero/modular": "^1.13.1",
    "@jaspero/modular-components": "^1.10.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jaspero/modular": "1.13.1",
    "@jaspero/modular-components": "1.10.0",
    "@playwright/test": "1.35.1",
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "1.20.5",
    "@sveltejs/package": "2.1.0",
    "@sveltejs/vite-plugin-svelte": "2.4.2",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte": "2.31.1",
    "gh-pages": "5.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "publint": "0.1.12",
    "sass": "^1.63.6",
    "svelte": "4.0.0",
    "svelte-check": "3.4.4",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
