{
  "name": "@jaspero/page-builder",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "concurrently \"npm run dev:demo\" \"npm run dev:lib\"",
    "dev:demo": "vite --config vite.demo.config.ts",
    "dev:lib": "vite build --watch",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build -s",
    "format": "prettier --plugin-search-dir . --write .",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist/lib"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@jaspero/modular": "^1.13.1",
    "@jaspero/modular-components": "^1.10.0",
    "@jaspero/web-components": "1.44.6"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "2.4.2",
    "@tsconfig/svelte": "5.0.0",
    "autoprefixer": "10.4.14",
    "concurrently": "8.2.0",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-svelte": "3.0.0",
    "svelte": "4.1.1",
    "svelte-check": "3.4.6",
    "svelte-preprocess": "5.0.4",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.1",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-plugin-static-copy": "0.17.0",
    "vitest": "0.32.2"
  },
  "type": "module"
}
