{
  "name": "@jaspero/page-builder",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "concurrently \"npm run dev:demo\" \"npm run dev:lib\"",
    "dev:demo": "vite --config vite.demo.config.ts",
    "dev:lib": "vite build --watch",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build -s",
    "format": "prettier --plugin-search-dir . --write .",
    "semantic-release": "semantic-release"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@jaspero/modular": "^1.13.1",
    "@jaspero/modular-components": "^1.10.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jaspero/modular": "1.13.1",
    "@jaspero/modular-components": "1.10.0",
    "@playwright/test": "1.35.1",
    "@sveltejs/adapter-static": "2.0.2",
    "svelte-preprocess": "5.0.4",
    "concurrently": "8.2.0",
    "vite-plugin-static-copy": "0.17.0",
    "@sveltejs/package": "2.1.0",
    "@sveltejs/vite-plugin-svelte": "2.4.2",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte": "2.31.1",
    "gh-pages": "5.0.0",
    "postcss": "8.4.27",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "publint": "0.1.12",
    "sass": "^1.63.6",
    "svelte": "4.0.0",
    "svelte-check": "3.4.4",
    "tailwindcss": "^3.3.3",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
